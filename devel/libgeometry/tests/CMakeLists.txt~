add_executable( geo_surface geo_surface.c )
target_link_libraries( geo_surface ert_geometry )

add_test( geo_surface_true ${EXECUTABLE_OUTPUT_PATH}/geo_surface
                           ${PROJECT_SOURCE_DIR}/test-data/Statoil/Geometry/Surface.irap TRUE )
add_test( geo_surface_false ${EXECUTABLE_OUTPUT_PATH}/geo_surface
                            /tmp/does/not/exist.irap FALSE )

set_property( TEST geo_surface_true   PROPERTY LABELS Statoil )
               
